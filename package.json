{
  "name": "milky-way-admin-panel",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^1.11.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "packageManager": "npm@10.2.5",
  "engines": {
    "node": ">=18.0.0"
  }
}
